{
    "name": "URL 연속 실행기",
    "description": "Load url when current page meet a certain condition.",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting", "tabs"],
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "https://*/*"
    ]
  }
